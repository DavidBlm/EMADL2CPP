package letters;

component LettersComposed{
    ports in Q(0:1)^{1, 28, 28} data,
         out Q(0:1)^{26} softmax;

    instance LettersInputLayer lettersinputlayer;
    instance LettersClassifier lettersclassifier;

    connect data -> lettersinputlayer.data;
    connect lettersinputlayer.vector -> lettersclassifier.vector;
    connect lettersclassifier.softmax -> softmax;

}
