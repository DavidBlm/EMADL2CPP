package digitsLoad;


component DigitsLoad {
       ports in Z(0:255)^{1, 28, 28} data,
             out Q(0:1)^{10} softmax;

      implementation CNN {
              data ->
              LoadNetwork(networkDir="model/digitsLoad.DigitsLoad", networkPrefix="newest-", numInputs=1, outputShape=10, elementTypeLowerBound="0", elementTypeUpperBound="1") ->
              softmax;
          }
}

