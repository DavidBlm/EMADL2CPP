package vae;

component Decoder{
    ports in Q(-1000:1000)^{8} encoding,
          out Q(-1000:1000)^{1,28,28} data;

    implementation CNN {
        encoding ->
        FullyConnected(units=200) ->
        Relu() ->
        Dropout() ->
        FullyConnected(units=400) ->
        Relu() ->
        Dropout() ->
        FullyConnected(units=784) ->
        Reshape(shape=(1,28,28)) ->
        Sigmoid() ->
        data;
    }
}