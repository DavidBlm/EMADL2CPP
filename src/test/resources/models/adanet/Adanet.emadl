component Adanet<classes=10, layerWidth=32, imageSize=32>{
    ports in Z(0:255)^{1, imageSize, imageSize} image,
          out Q(0:1)^{classes} predictions;

    implementation CNN {
        image ->
        FullyConnected(| = 3, units=layerWidth)->
        Concatenate() ->
        FullyConnected(units=classes) ->
        predictions;
    }
}